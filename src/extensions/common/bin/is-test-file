#! /bin/sh

[ "${TRACE}" = "YES" ] && set -x && : "$0" "$@"

case "$1" in
   -l|--list-test-extensions)
      echo "stdout ccdiag stderr stdin Makefile sh"
      exit 0
   ;;

   *)
      # whatever
   ;;
esac


[ $# -ne 1 ] && exit 1


case "$1" in
   *\.ccdiag|*\.stdin|*\.stdout|*\.stderr|Makefile|\*.sh)
      echo "${TEST_YES}"
      exit 0
   ;;
esac

echo "NO"
